
@model object

@{
    ViewBag.Title = "加工记录";
    Layout = "_Layout";
}
<script src="~/mjs/StatisticManagement/Index.js"></script>
<section class="content">
    <h2 class="text-center">加工记录统计</h2>
    <div class="box box-primary">
        <div class="box-header">
            <h3 class="box-title">加工记录</h3>
            <div class="box-tools pull-right">
                <button type="button" class="btn btn-primary" onclick="getStatisticList()">重新加载</button>
                <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            </div>
        </div>
        <div class="box-body">
                <div id="main" class="" style="border: 1px solid red; width: 100%; height: 500px"></div>
        </div>
    </div>
</section>