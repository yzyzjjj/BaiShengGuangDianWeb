                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- 日志模态框 -->
    <div class="modal fade" tabindex="-1" id="logModal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;
                    </button>
                    <h4 class="modal-title" id="logTitle"></h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label class="control-label" for="logStartDate">时间范围：</label>
                        <div class="form-group no-margin" style="display: -webkit-inline-box">
                            <input type="text" id="logStartDate" class="form_date form-control pointer text-center" style="width: 130px">
                            <span style="display: table-cell; vertical-align: middle; height: 33px">&nbsp;-&nbsp;</span>
                            <input type="text" id="logEndDate" class="form_date form-control pointer text-center" style="width: 130px">
                        </div>
                    </div>
                    <div class="form-group" id="logPlanBox">
                        <label class="control-label" for="logBillSelect">计划号：</label>
                        <div class="form-group no-margin" style="display: -webkit-inline-box">
                            <select class="ms2 form-control" id="logPlanSelect" style="width: 160px"></select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="logBillSelect">货品编号：</label>
                        <div class="form-group no-margin" style="display: -webkit-inline-box">
                            <select class="ms2 form-control" id="logBillSelect" style="width: 160px"></select>
                        </div>
                    </div>
                    <div class="form-group" id="billInfo">
                        <div class="flexStyle" style="flex-wrap:wrap">
                            <label class="control-label textOverTop" for="logCategory">货品类别：<span class="spanStyle" id="logCategory"></span></label>
                            <label class="control-label textOverTop" for="logName">货品名称：<span class="spanStyle" id="logName"></span></label>
                            <label class="control-label textOverTop" for="logSupplier">供应商：<span class="spanStyle" id="logSupplier"></span></label>
                            <label class="control-label textOverTop" for="logSpecification">规格型号：<span class="spanStyle" id="logSpecification"></span></label>
                            <label class="control-label textOverTop" for="logPrice">单价：<span class="spanStyle" id="logPrice"></span></label>
                        </div>
                    </div>
                    <div class="table-responsive mailbox-messages">
                        <table class="table table-hover table-striped" id="logList"></table>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                </div>
            </div>
        </div>
    </div>
