@model object

@{
    ViewBag.Title = "用户管理";
    Layout = "_Layout";
}

<script src="~/mjs/AccountManagement/users.js"></script>
<link href="~/mcss/accountManagement.css" rel="stylesheet" />	
<div class="title"><h2>用户管理</h2></div>	
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <button type="button" class="btn btn-primary pull-left" onclick="getSiteList()" id="re_load">重新加载</button>
            <button type="button" class="btn btn-primary pull-left" onclick="showAddSite()" id="styles_id">添加用户</button>
        </div>
        <div class="box-body">
            <div class="table-responsive mailbox-messages">
                <table class="table table-hover table-striped" id="userTable"></table>
            </div>
        </div>
    </div>
    
</section>

