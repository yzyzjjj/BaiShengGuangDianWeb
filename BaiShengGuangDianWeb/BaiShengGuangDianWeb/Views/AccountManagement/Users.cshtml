@model object

@{
    ViewBag.Title = "用户管理";
    Layout = "_Layout";
}

<script src="~/mjs/AccountManagement/users.js"></script>
<link href="~/mcss/accountManagement.css" rel="stylesheet" />	
<div class="title" style="background: url('mimages/righttitlebig.png') repeat-x; border: 1px solid #c1d3de; margin: 0; padding: 0;overflow: visible;"><h2 style="margin: 0;margin: 0;padding: 0; line-height: 33px;margin-left: 2px;font-weight: bold; font-size: 16px;font-family: '微软雅黑'; color: #000000; ">用户管理</h2></div>	
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <button type="button" class="btn btn-primary pull-left" onclick="getSiteList()" id="re_load">重新加载</button>
            <button type="button" class="btn btn-primary pull-left" onclick="showAddSite()" id="styles_id">添加用户</button>
        </div>
        <div class="box-body">
            <div class="table-responsive mailbox-messages">
                <table class="table table-hover table-striped" id="userTable"></table>
            </div>
        </div>
    </div>
    
</section>

