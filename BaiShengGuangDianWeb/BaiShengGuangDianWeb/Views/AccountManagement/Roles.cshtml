@model object

@{
    ViewBag.Title = "角色管理";
    Layout = "_Layout";
}
<script src="~/mjs/AccountManagement/roles.js"></script>
<link href="~/mcss/accountManagement.css" rel="stylesheet" />		
<div class="title"><h2>角色管理</h2></div>		
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">角色</h3>
            <button type="button" class="btn btn-primary pull-right" onclick="showAddModel()">添加新角色</button>
        </div>
        <div class="box-body">
            <div class="table-responsive mailbox-messages">
                <table class="table table-hover table-striped" id="rolesList"></table>
            </div>
        </div>
    </div>
</section>

